{% extends 'base.html.twig' %}

{% block title %}Hello UtilisateurController!{% endblock %}

{% block body %}
<h1>Inscription</h1>
<div class="container">
    <div class="form-group col-md-4">
        {{form_start(form)}}
        {{form_row(form.nom)}}
        {{form_row(form.prenom)}}
        {{form_row(form.mdp)}}
        <button type="submit" class="btn btn-primary">s'inscrire</button>
        {{form_end(form)}}

    </div>
</div>
{% endblock %}